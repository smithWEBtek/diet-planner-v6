<table>
  <tr align="left">
    <th>Edit</th>
    <th>Delete</th>
    <th>ID</th>
    <th>Celeb</th>
    <th>Verb</th>
    <th>Food</th>
    <th>Phrase</th>
    <th>Diet</th>
  </tr>
  <% users.sort_by.each do |user| %>
  <tr>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Delete', user_path(user), :method => :delete, :confirm => 'Are you sure?' %></td>
    <td><%= quote.id %></td>
    <td><%= quote.celeb.name %></td>
    <td><%= quote.verb.name %></td>
    <td><%= quote.food.name %></td>
    <td><%= link_to quote.diet.name, diet_path(quote.diet) %></td>
  </tr>
  <% end %>
</table>
